[Unit]
Description=Atari PAC-MAN Hi-scores
After=remote-fs.target
After=syslog.target

# replace /tnfs with your TNFS directory

[Service]
User=root
Group=root
ExecStart=/usr/local/sbin/dkong "/tnfs/Atari_8-bit/Games/High Score Enabled/Donkey Kong.atr" "/var/www/atari-scores/dkong.html"

[Install]
WantedBy=multi-user.target
