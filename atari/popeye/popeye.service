[Unit]
Description=Atari Popeye Hi-scores
After=remote-fs.target
After=syslog.target

# replace /tnfs with your TNFS directory

[Service]
User=root
Group=root
ExecStart=/usr/local/sbin/popeye "/tnfs/Atari_8-bit/Games/High Score Enabled/Popeye.atr" "/scores/popeye.html"

[Install]
WantedBy=multi-user.target
